{
  "editor.find.addExtraSpaceOnTop": false,
  "editor.formatOnSave": true,
  "editor.snippetSuggestions": "top",
  "terminal.external.osxExec": "iTerm.app",
  "colorize.languages": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "sss",
    "stylus",
    "svg",
    "tsx",
    "typescript",
    "xml"
  ],
  "window.title": "${activeEditorFull}${separator}${rootName}",
  "editor.tabSize": 2,
  "terminal.integrated.scrollback": 10000,
  "files.trimTrailingWhitespace": true,
  "editor.renderWhitespace": "none",
  "diffEditor.renderSideBySide": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "workbench.startupEditor": "newUntitledFile",
  "gitlens.menus": {
    "editor": {
      "remote": false,
      "clipboard": true,
      "compare": true,
      "blame": false,
      "history": false
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": true
    },
    "scm": {
      "authors": true
    },
    "explorer": {
      "remote": true,
      "clipboard": true,
      "compare": true,
      "history": true
    },
    "scmItem": {
      "remote": false,
      "history": true,
      "clipboard": true,
      "compare": true,
      "stash": true
    },
    "scmGroupInline": {
      "stash": true
    },
    "editorTab": {
      "remote": true,
      "clipboard": true,
      "compare": true,
      "history": true
    }
  },
  "editor.trimAutoWhitespace": true,
  "editor.accessibilitySupport": "off",
  "explorer.confirmDelete": false,
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "editor.formatOnPaste": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb",
    "git-rebase-todo": "gitlens.rebase"
  },
  "sortJSON.excludedFiles": ["package.json"],
  "git.autofetch": true,
  "files.insertFinalNewline": true,
  "editor.minimap.enabled": false,
  "explorer.confirmDragAndDrop": false,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "git.confirmSync": false,
  "settingsSync.keybindingsPerPlatform": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "json.format.enable": false,
  "html.format.enable": false,
  "less.format.enable": false,
  "scss.format.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "css.format.enable": false,
  "workbench.iconTheme": "vscode-icons",
  "gitlens.rebaseEditor.ordering": "asc",
  "[python]": {
    "editor.formatOnType": true
  },
  "vsicons.dontShowNewVersionMessage": true,
  "[njk]": {
    "editor.defaultFormatter": "eseom.nunjucks-template"
  },
  "terminal.integrated.tabs.enabled": false,
  "workbench.colorTheme": "GitHub Dark Default",
  "prisma.showPrismaDataPlatformNotification": false,
  "github.copilot.editor.enableAutoCompletions": true
}
